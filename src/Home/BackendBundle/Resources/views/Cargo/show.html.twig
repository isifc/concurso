{% extends 'MWSimpleCrudGeneratorBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - {{ 'views.show.show'|trans({'%entity%': 'Cargo'}, 'MWSimpleCrudGeneratorBundle') }}
{% endblock %}

{% block page %}
    
<div class="well well-lg">
    <h1>Cargo</h1>
    <form class="form-horizontal">
        <fieldset>                <div class="form-group">
                    <label class="control-label">Id</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.id }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Categoria</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.categoria }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Apartado</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.apartado }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Escalafon</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.escalafon }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Funcion</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.funcion }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Denominacion</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.denominacion }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Grupo</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.grupo }}</span>                    </div>
                </div>                <div class="form-group">
                    <label class="control-label">Cuof</label>
                    <div class="form-control">                        <span class="input-xlarge uneditable-input">{{ entity.cUOF }}</span>                    </div>
                </div>        </fieldset>
    </form>
</div>

        <div class="col-lg-9">
    <div class="float-left col-lg-3">
        <a class="btn btn-info col-lg-11" href="{{ path('admin_cargo') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'MWSimpleCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left col-lg-3">
        <a class="btn btn-primary col-lg-11" href="{{ path('admin_cargo_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'MWSimpleCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left col-lg-3">{{ form(delete_form) }}</div>
</div>
{% endblock %}
