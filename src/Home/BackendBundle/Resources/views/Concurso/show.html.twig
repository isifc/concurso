{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Concurso</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Tipoconcurso</th>
                <td>{{ entity.tipoConcurso }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{{ entity.fechaInicio|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{{ entity.fechaFin|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Dectreto</th>
                <td>{{ entity.dectreto }}</td>
            </tr>
            <tr>
                <th>Cantidadcargo</th>
                <td>{{ entity.cantidadCargo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('concurso') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
